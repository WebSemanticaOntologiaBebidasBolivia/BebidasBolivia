@prefix :        <#> .
@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .

[] rdf:type fuseki:Server ;
   fuseki:services (
     :service-bebidas
   ) .

:service-bebidas a fuseki:Service ;
  fuseki:name "bebidas" ;
  fuseki:serviceQuery "sparql" ;
  fuseki:serviceUpdate "update" ;
  fuseki:serviceUpload "upload" ;
  fuseki:serviceReadWriteGraphStore "data" ;
  fuseki:dataset :dataset-bebidas .

:dataset-bebidas a ja:MemoryDataset ;
  ja:defaultGraph [
    a ja:Graph ;
    ja:content [
      a ja:TextFile ;
      ja:externalContent "/fuseki/bebidasbolivia.ttl"
    ]
  ] .
